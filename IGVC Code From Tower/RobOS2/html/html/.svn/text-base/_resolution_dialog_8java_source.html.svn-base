<a name="l00014"></a>00014 
<a name="l00015"></a><a class="code" href="classrob_o_s2_1_1gui_1_1simulator_1_1_resolution_dialog.html">00015</a> <span class="keyword">public</span> <span class="keyword">class </span><a class="code" href="classrob_o_s2_1_1gui_1_1simulator_1_1_resolution_dialog.html">ResolutionDialog</a> {
<a name="l00016"></a><a class="code" href="classrob_o_s2_1_1gui_1_1simulator_1_1_resolution_dialog.html#a1a7065811679754f58f4de3056bbb695">00016</a>         <span class="keyword">static</span> Logger <a class="code" href="classrob_o_s2_1_1gui_1_1simulator_1_1_resolution_dialog.html#a1a7065811679754f58f4de3056bbb695">log</a> = Logger.getLogger(<a class="code" href="classrob_o_s2_1_1gui_1_1simulator_1_1_velocity_dialog.html">VelocityDialog</a>.class.getName());
<a name="l00017"></a>00017         
<a name="l00018"></a><a class="code" href="classrob_o_s2_1_1gui_1_1simulator_1_1_resolution_dialog.html#a4b70f46a6fedb10c1b94ec516f89dee7">00018</a>         <span class="keyword">public</span> <a class="code" href="classrob_o_s2_1_1gui_1_1simulator_1_1_resolution_dialog.html#a4b70f46a6fedb10c1b94ec516f89dee7">ResolutionDialog</a>(Shell parentShell, <span class="keyword">final</span> <a class="code" href="classrob_o_s2_1_1simulator_1_1_engine.html">Engine</a> engine) {
<a name="l00019"></a>00019                 <span class="comment">//Create a new dialog box &amp; bind it to the parent shell</span>
<a name="l00020"></a>00020                 <span class="keyword">final</span> Shell dialog = <span class="keyword">new</span> Shell (parentShell, SWT.DIALOG_TRIM | SWT.APPLICATION_MODAL);
<a name="l00021"></a>00021                 dialog.setText(<span class="stringliteral">&quot;Simulator Resolution&quot;</span>);
<a name="l00022"></a>00022                 dialog.setLayout (<span class="keyword">new</span> RowLayout ());
<a name="l00023"></a>00023                 
<a name="l00024"></a>00024                 Label currVel = <span class="keyword">new</span> Label(dialog, SWT.NONE);
<a name="l00025"></a>00025                 currVel.setText(<span class="stringliteral">&quot;Current Resolution: &quot;</span> + engine.<a class="code" href="classrob_o_s2_1_1simulator_1_1_engine.html#a334e44720bed1aa4e5cde11cec30d099">getResolution</a>() + <span class="stringliteral">&quot; cm    &quot;</span>);
<a name="l00026"></a>00026                 
<a name="l00027"></a>00027                 <span class="comment">//Create a label stating what we are looking for</span>
<a name="l00028"></a>00028                 Label label = <span class="keyword">new</span> Label(dialog, SWT.NONE);
<a name="l00029"></a>00029                 label.setText(<span class="stringliteral">&quot;Desired Resolution: &quot;</span>);
<a name="l00030"></a>00030                 
<a name="l00031"></a>00031                 <span class="comment">//Text input area</span>
<a name="l00032"></a>00032                 <span class="keyword">final</span> Text text = <span class="keyword">new</span> Text(dialog, SWT.BORDER);
<a name="l00033"></a>00033                 
<a name="l00034"></a>00034                 <span class="comment">//Don&#39;t forget the units!!!</span>
<a name="l00035"></a>00035                 Label ms = <span class="keyword">new</span> Label(dialog, SWT.NONE);
<a name="l00036"></a>00036                 ms.setText(<span class="stringliteral">&quot;cm&quot;</span>);
<a name="l00037"></a>00037                 
<a name="l00038"></a>00038                 <span class="comment">//OK &amp; Cancel Buttons</span>
<a name="l00039"></a>00039                 <span class="keyword">final</span> Button ok = <span class="keyword">new</span> Button (dialog, SWT.PUSH);
<a name="l00040"></a>00040                 ok.setText (<span class="stringliteral">&quot;OK&quot;</span>);
<a name="l00041"></a>00041                 Button cancel = <span class="keyword">new</span> Button (dialog, SWT.PUSH);
<a name="l00042"></a>00042                 cancel.setText (<span class="stringliteral">&quot;Cancel&quot;</span>);
<a name="l00043"></a>00043                 
<a name="l00044"></a>00044                 <span class="comment">//Button Listener</span>
<a name="l00045"></a>00045                 Listener listener =<span class="keyword">new</span> Listener () {
<a name="l00046"></a>00046                         <span class="keyword">public</span> <span class="keywordtype">void</span> handleEvent (Event event) {
<a name="l00047"></a>00047                                 <span class="keywordflow">if</span>(event.widget == ok){
<a name="l00048"></a>00048                                         <span class="comment">//OK button clicked. Commit new velocity and close dialog.</span>
<a name="l00049"></a>00049                                         <span class="keywordflow">try</span>{
<a name="l00050"></a>00050                                                 <span class="keywordtype">int</span> res = Integer.parseInt(text.getText());
<a name="l00051"></a>00051                                                 engine.<a class="code" href="classrob_o_s2_1_1simulator_1_1_engine.html#a4d68322c41e7a1e8f7d4d4e33554d5ed">setResolution</a>(res);
<a name="l00052"></a>00052                                         } <span class="keywordflow">catch</span> (Exception e){
<a name="l00053"></a>00053                                                 <a class="code" href="classrob_o_s2_1_1gui_1_1simulator_1_1_resolution_dialog.html#a1a7065811679754f58f4de3056bbb695">log</a>.error(<span class="stringliteral">&quot;Could Not Set the Resolution. Please try and set it again&quot;</span>);
<a name="l00054"></a>00054                                         }
<a name="l00055"></a>00055                                 }
<a name="l00056"></a>00056                                 
<a name="l00057"></a>00057                                 <span class="comment">//Close the dialog box</span>
<a name="l00058"></a>00058                                 dialog.close ();
<a name="l00059"></a>00059                         }
<a name="l00060"></a>00060                 };
<a name="l00061"></a>00061                 
<a name="l00062"></a>00062                 <span class="comment">//Add the listeners</span>
<a name="l00063"></a>00063                 ok.addListener (SWT.Selection, listener);
<a name="l00064"></a>00064                 cancel.addListener (SWT.Selection, listener);
<a name="l00065"></a>00065                 
<a name="l00066"></a>00066                 <span class="comment">//Make the dialog the correct size and open it</span>
<a name="l00067"></a>00067                 dialog.pack ();
<a name="l00068"></a>00068                 dialog.open ();
<a name="l00069"></a>00069 
<a name="l00070"></a>00070         }
<a name="l00071"></a>00071 }
